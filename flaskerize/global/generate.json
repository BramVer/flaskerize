{
  "options": [
    {
      "arg": "what",
      "type": "str",
      "help": "What to generate"
    },
    {
      "arg": "output_name",
      "type": "str",
      "default": null,
      "help": "Base name for outputted resource"
    },
    {
      "arg": "--output-file",
      "aliases": ["-o"],
      "type": "str",
      "help": "Name of output file"
    },
    {
      "arg": "--dry-run",
      "action": "store_true",
      "help": "Dry run -- don't actually create any files."
    }
  ]
}
